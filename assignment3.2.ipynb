    "def rev(x):\n",
    "    l = \"\"\n",
    "    for i in range(len(x)-1,-1,-1):\n",
    "        l = l+x[i]\n",
    "    return l\n",
    "a = input(\"enter string\")\n",
    "print(a)\n",
    "print(\"reverse is:\",rev(a))"
